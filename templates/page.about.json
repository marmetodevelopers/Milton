    "bac5623c-3785-448b-b03a-ea0ec5e42793": {
      "type": "rich-text",
      "blocks": {
        "template--15936832569444__bac5623c-3785-448b-b03a-ea0ec5e42793-heading-1": {
          "type": "heading",
          "settings": {
            "title": "Vision",
            "heading_size": "h1"
          }
        },
        "template--15936832569444__bac5623c-3785-448b-b03a-ea0ec5e42793-text-1": {
          "type": "text",
          "settings": {
            "enlarge_text": true,
            "text": "<p>To be the unassailable leader in homewares by providing ‘Best-in-Class’ products & solutions that redefine value, convenience and style.<\/p>"
          }
        }
      },
      "block_order": [
        "template--15936832569444__bac5623c-3785-448b-b03a-ea0ec5e42793-heading-1",

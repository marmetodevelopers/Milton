    "3bd94ea2-48c5-4d31-9272-66bed2eb5a01": {
      "type": "slideshow-split",
      "blocks": {
        "template--15936832569444__3bd94ea2-48c5-4d31-9272-66bed2eb5a01-slide-1": {
          "type": "slide",
          "settings": {
            "top_subheading": "Kensington Palace, London, 2019",
            "title": "World Branding Awards",
            "title_size": 30,
            "subheading": "The World Branding Awards is the premier awards of the World Branding Forum. Milton being named ‘Brand of the Year’ is an illustrious and reputable achievement.",
            "link": "",
            "link_text": "",
            "link_2": "",
            "link_text_2": "",
